        </div>
      </div>
      <div class="control-group">
        <label class="control-label" for="meetingRoom">会议室</label>
        <div class="controls">
          <select id="meetingRoom" name="meetingRoom">
                <option value="">-- 请选择会议室 --</option>
                <% for(var i = 0; i < meeting_rooms.length; i++) {
                  var meeting_room = meeting_rooms[i]; %>
								<option value="<%= meeting_room.name %>"><%= meeting_room.name %></option>
                <% } %>
							</select>
          <span class="help-inline"></span>
        </div>
      </div>
      <div class="control-group">
        <label class="control-label" for="title">会议主题</label>
        <div class="controls">
          <input type="text" id="title" name="title">
          <span class="help-inline"></span>
        </div>
      </div>
      <div class="control-group">
        <label class="control-label" for="department">部门</label>
        <div class="controls">
							            <select id="department" name="department">
								            <option value="">-- 请选择部门 --</option>
                            <% for(var i = 0; i < departments.length; i++) {
                                var department = departments[i]; %>
								            <option value="<%= department.name %>"><%= department.name %></option>
								            <% } %>
							            </select>

          <span class="help-inline"></span>
        </div>
      </div>
      <div class="control-group">
        <label class="control-label" for="username">人名</label>
        <div class="controls">
          <input type="text" id="username" name="username">
          <span class="help-inline"></span>
        </div>
      </div>
      <div class="control-group">
        <label class="control-label" for="phone">分机</label>
        <div class="controls">
                                     <input type="text" id="phone" name="phone">
          <span class="help-inline"></span>
        </div>
      </div>
      <div class="control-group">
        <label class="control-label" for="selectExpressCompany">设备</label>
        <div class="controls">
                          <input type="button" id="equipment" value="选择设备" style="pointer:corsor">
                    <span id="equipmentList"></span>
          <span class="help-inline"></span>
        </div>
      </div>
</form>